# {{ ansible_managed }}

configDirectory={{ pihole_config_path }}
installDirectory={{ pihole_install_path }}
PIHOLE_INTERFACE={{ pihole_network_interface }}
IPV4_ADDRESS={{ pihole_ipv4_address }}/24
PIHOLE_DNS_1={{ pihole_dns_servers[0] }}
